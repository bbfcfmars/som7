{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/spa"
      }
    }
  ],
   "routes": [                                                                   
    {                                                                           
      "src": "/(.*)",                                                           
      "dest": "/dist/node-build.mjs"                                            
    }                                                                           
  ]                        
}
