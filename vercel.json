{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/server/production.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/server/production.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server/production.mjs"
    }
  ]
}
